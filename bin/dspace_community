#! /bin/bash
#
# Get information about DSpace communities in tabular form.
#
# Usage: dspace_community [DEPLOYMENT] [OPTIONS]
# Usage: dspace_community --help

PROGRAM=`realpath "$0"`
BIN_DIR=`dirname "$PROGRAM"`
PROGNAME=`basename "$PROGRAM"`

source "$BIN_DIR/dspace_values"

exec "$BIN_DIR/run_ruby" "$PROGNAME" "$@"
