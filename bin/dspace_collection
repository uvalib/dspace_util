#! /bin/bash
#
# Get information about DSpace Collections in tabular form.
#
# Usage: dspace_collection [DEPLOYMENT] [OPTIONS]
# Usage: dspace_collection --help

PROGRAM=`realpath "$0"`
BIN_DIR=`dirname "$PROGRAM"`
PROGNAME=`basename "$PROGRAM"`

source "$BIN_DIR/dspace_values"

exec "$BIN_DIR/run_ruby" "$PROGNAME" "$@"
